<?xml version="1.0"?>
<tryton>
    <data>
        <record model="ir.ui.view" id="sale_form">
            <field name="model">stock.shipment.out</field>
            <field name="inherit" ref="stock.shipment_out_view_form"/>
            <field name="name">stock_form</field>
        </record>
           <record model="ir.ui.view" id="stock_incoterm_view_form">
            <field name="model">stock.shipment.out.incoterm</field>
            <field name="type">form</field>
            <field name="name">stock_incoterm_form</field>
        </record>
        <record model="ir.ui.view" id="stock_incoterm_view_tree">
            <field name="model">stock.shipment.out.incoterm</field>
            <field name="type">tree</field>
            <field name="name">stock_incoterm_tree</field>
        </record>
        <record model="ir.action.act_window" id="act_stock_incoterm_form">
            <field name="name">Stock Shipment Incoterm</field>
            <field name="res_model">stock.shipment.out.incoterm</field>
        </record>
           <record model="ir.action.act_window.view" id="act_stock_incoterm_form_view1">
            <field name="sequence" eval="10"/>
            <field name="view" ref="stock_incoterm_view_tree"/>
            <field name="act_window" ref="act_stock_incoterm_form"/>
        </record>
          <record model="ir.action.act_window.view" id="act_stock_incoterm_form_view2">
            <field name="sequence" eval="20"/>
            <field name="view" ref="stock_incoterm_view_form"/>
            <field name="act_window" ref="act_stock_incoterm_form"/>
        </record>
         <menuitem parent="stock.menu_configuration" action="act_stock_incoterm_form"
            id="menu_stock_incoterm_form"/>   
    </data>
</tryton>
